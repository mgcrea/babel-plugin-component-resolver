{
  "name": "babel-plugin-component-resolver",
  "version": "1.0.3",
  "description": "Babel plugin to resolve components based on their dirname when an index file is not present",
  "main": "lib/index.js",
  "repository": "mgcrea/babel-plugin-relative-import",
  "author": "Olivier Louvignes <olivier@mg-crea.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run test:watch",
    "compile": "NODE_ENV=development babel src/ -d lib/ -s",
    "compile:watch": "npm run compile -- --watch",
    "test": "NODE_ENV=test mocha",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src/ test/",
    "prepublish": "rimraf lib/*; npm run compile"
  },
  "dependencies": {
    "find-pkg": "^0.1.2",
    "resolve": "^1.1.7"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "debug-utils": "^0.2.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "expect": "^1.20.2",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4"
  }
}
